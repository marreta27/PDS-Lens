{
  "manifest_version": 3,
  "name": "PDS Lens - Tableau Cloud Data Source Viewer",
  "version": "1.0.0",
  "description": "View Public Data Sources from Tableau Cloud using PAT authentication",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.online.tableau.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
